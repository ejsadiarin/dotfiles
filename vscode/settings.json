{
  "github.copilot.nextEditSuggestions.enabled": true,
  "window.menuBarVisibility": "toggle",
  "editor.fontFamily": "'IosevkaTerm Nerd Font Mono', 'monospace', monospace",
  "editor.fontSize": 18,
  "security.workspace.trust.startupPrompt": "never",
  "security.workspace.trust.untrustedFiles": "newWindow",
  "security.workspace.trust.enabled": false,
  "workbench.sideBar.location": "left",
  "editor.minimap.side": "right",
  "editor.minimap.scale": 2,
  "vim.easymotion": true,
  "vim.incsearch": true,
  "vim.useSystemClipboard": true,
  "vim.useCtrlKeys": true,
  "vim.hlsearch": true,
  "vim.highlightedyank.enable": true,
  "vim.highlightedyank.color": "rgba(250, 240, 170, 0.5)",
  "vim.cursorStylePerMode.insert": "block",
  "vim.cursorStylePerMode.normal": "block",
  "oil-code.disableVimKeymaps": true,
  "oil-code.hasNerdFont": true,
  "vim.insertModeKeyBindings": [
    {
      "before": [
        "k",
        "j"
      ],
      "after": [
        "<Esc>"
      ]
    },
    {
      "before": [
        "<C-a>"
      ],
      "commands": [
        "editor.action.selectAll"
      ]
    }
  ],
  "vim.normalModeKeyBindingsNonRecursive": [
    {
      "before": [
        "<Esc>"
      ],
      "commands": [
        ":nohl"
      ]
    },
    {
      "before": [
        "<leader>",
        "b",
        "d"
      ],
      "commands": [
        ":bd"
      ]
    },
    {
      "before": [
        "H"
      ],
      "commands": [
        "workbench.action.previousEditor"
      ]
    },
    {
      "before": [
        "L"
      ],
      "commands": [
        "workbench.action.nextEditor"
      ]
    },
    {
      "before": [
        "K"
      ],
      "commands": [
        {
          "command": "editor.action.showHover"
        }
      ]
    },
    {
      "before": [
        "<leader>",
        "r",
        "n"
      ],
      "commands": [
        {
          "command": "editor.action.rename"
        }
      ]
    },
    {
      "before": [
        "<leader>",
        "c",
        "r"
      ],
      "commands": [
        {
          "command": "editor.action.rename"
        }
      ]
    },
    {
      "before": [
        "<leader>",
        "c",
        "a"
      ],
      "commands": [
        {
          "command": "editor.action.codeAction"
        }
      ]
    },
    {
      "before": [
        "<leader>",
        "e"
      ],
      "commands": [
        "workbench.action.toggleSidebarVisibility"
      ]
    },
    {
      "before": [
        "<leader>",
        "t"
      ],
      "commands": [
        "workbench.action.terminal.toggleTerminal"
      ]
    },
    {
      "before": [
        "<leader>",
        "f",
        "f"
      ],
      "commands": [
        "workbench.action.quickOpen"
      ]
    },
    {
      "before": [
        "<leader>",
        "f",
        "e"
      ],
      "commands": [
        "workbench.action.quickOpen"
      ]
    },
    {
      "before": [
        "<leader>",
        "f",
        "r"
      ],
      "commands": [
        "workbench.action.openRecent"
      ]
    },
    {
      "before": [
        "<leader>",
        "s",
        "g"
      ],
      "commands": [
        "workbench.action.findInFiles"
      ]
    },
    {
      "before": [
        "<leader>",
        "s",
        "s"
      ],
      "commands": [
        "workbench.action.showAllSymbols"
      ]
    },
    {
      "before": [
        "<leader>",
        "a",
        "a"
      ],
      "commands": [
        "workbench.action.toggleAuxiliaryBar",
        "github.copilot.chat.focus"
      ]
    },
    {
      "before": [
        "<leader>",
        "tab"
      ],
      "commands": [
        "workbench.action.quickOpenPreviousRecentlyUsedEditor",
        "workbench.action.acceptSelectedQuickOpenItem"
      ]
    },
    {
      "before": [
        "g",
        "r"
      ],
      "commands": [
        "findAllFileReferences"
      ]
    },
    {
      "before": [
        "g",
        "i"
      ],
      "commands": [
        "findImplementations"
      ]
    }
  ],
  "vim.visualModeKeyBindingsNonRecursive": [
    {
      "before": [
        "y"
      ],
      "after": [
        "y",
        "`",
        ">"
      ]
    }
  ],
  "vim.leader": "<space>",
  "vim.handleKeys": {
    "<C-f>": false,
    "<C-b>": false,
    "<C-S-l>": false,
    "<C-i>": false,
    "<C-y>": false
  },
  "settings.cycle": [
    {
      "id": "zenPerfection", // must be unique
      "overrideWorkspaceSettings": true,
      "values": [
        // Note: use the same settings in each values object
        {
          "workbench.editor.showTabs": "none",
          "breadcrumbs.enabled": true,
          "workbench.statusBar.visible": false,
          "workbench.activityBar.location": "hidden"
        },
        {
          "workbench.editor.showTabs": "multiple",
          "workbench.statusBar.visible": true,
          "workbench.activityBar.location": "side"
        }
      ]
    }
  ],
  // Enable smooth scrolling in the editor
  "editor.smoothScrolling": false,
  "editor.lineNumbers": "relative",
  "diffEditor.ignoreTrimWhitespace": false,
  "editor.minimap.enabled": false,
  "extensions.experimental.affinity": {
    "asvetliakov.vscode-neovim": 1
  },
  // "vscode-neovim.useWSL": true,
  // "vscode-neovim.neovimExecutablePaths.linux": "/home/exquisite/.neovim/squashfs-root/usr/bin/nvim",
  "editor.cursorSurroundingLines": 10,
  "discord.detailsEditing": "{empty}",
  "discord.detailsIdling": "{empty}",
  "discord.lowerDetailsDebugging": "{empty}",
  "discord.lowerDetailsIdling": "{empty}",
  "discord.removeDetails": true,
  "discord.removeLowerDetails": true,
  "discord.removeRemoteRepository": true,
  "glassit.alpha": 220,
  "vsicons.dontShowNewVersionMessage": true,
  "workbench.iconTheme": "vscode-icons",
  "sqltools.useNodeRuntime": false,
  "terminal.integrated.env.linux": {},
  "keyboard.dispatch": "keyCode",
  "editor.tabSize": 4,
  "editor.detectIndentation": false,
  "editor.formatOnSave": true,
  "search.useParentIgnoreFiles": true,
  "search.useIgnoreFiles": true,
  "files.exclude": {
    "**/.git": true,
    "**/.svn": true,
    "**/.hg": true,
    "**/CVS": true,
    "**/.DS_Store": true,
    "**/Thumbs.db": true
  },
  "search.exclude": {
    "**/node_modules": true,
    "**/bower_components": true,
    "**/*.code-search": true
  },
  "files.watcherExclude": {
    "**/.git/objects/**": true,
    "**/.git/subtree-cache/**": true,
    "**/node_modules/*/**": true,
    "**/.hg/store/**": true
  },
  "[typescript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[typescriptreact]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "sqltools.disableNodeDetectNotifications": true,
  "github.copilot.enable": {
    "*": false,
    "plaintext": false,
    "markdown": false,
    "scminput": false
  },
  "[jsonc]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "workbench.editor.showTabs": "none",
  "window.commandCenter": false,
  "redhat.telemetry.enabled": false,
  "workbench.colorTheme": "Bearded Theme Monokai Black",
  "breadcrumbs.enabled": true,
  "vim.normalModeKeyBindings": [
    {
      "before": [
        "<leader>",
        "-"
      ],
      "commands": [
        {
          "command": "oil-code.open"
        }
      ]
    },
    {
      "before": [
        "<cr>"
      ],
      "commands": [
        {
          "command": "oil-code.select"
        }
      ]
    },
    {
      "before": [
        "<c-l>"
      ],
      "commands": [
        {
          "command": "oil-code.refresh"
        }
      ]
    },
    {
      "before": [
        "<c-c>"
      ],
      "commands": [
        {
          "command": "oil-code.close"
        }
      ]
    },
    {
      "before": [
        "`"
      ],
      "commands": [
        {
          "command": "oil-code.cd"
        }
      ]
    }
  ],
  "workbench.activityBar.location": "hidden"
}
